# RacePrep Environment Variables Template
# Copy this file to .env.local for local development
# For production, set these variables in your Netlify dashboard

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:3000/api
EXPO_PUBLIC_ENVIRONMENT=development

# External API Keys (get real keys from providers)
EXPO_PUBLIC_CHRONOTRACK_API_KEY=your-chronotrack-api-key
EXPO_PUBLIC_RUNSIGNUP_API_KEY=your-runsignup-api-key
EXPO_PUBLIC_RUNSIGNUP_API_SECRET=your-runsignup-api-secret
EXPO_PUBLIC_OPENWEATHERMAP_API_KEY=your-openweathermap-api-key
EXPO_PUBLIC_STRAVA_CLIENT_ID=your-strava-client-id
EXPO_PUBLIC_STRAVA_CLIENT_SECRET=your-strava-client-secret
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# App Configuration
EXPO_PUBLIC_APP_NAME=RacePrep
EXPO_PUBLIC_APP_VERSION=1.0.0

# Production only (set in Netlify dashboard)
# EXPO_PUBLIC_APP_URL=https://raceprep.kineticbrandpartners.com